public class Ejercicio07 {

    public static boolean esPrimo(int n) {
        if (n <= 1) return false;
        for (int i = 2; i <= Math.sqrt(n); i++) {
            if (n % i == 0) return false;
        }
        return true;
    }

    public static void main(String[] args) {
        int n = 29;
        System.out.println(n + " Â¿es primo? " + esPrimo(n));
    }
}
